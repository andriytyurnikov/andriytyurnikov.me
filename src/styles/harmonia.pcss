@import "./lib/canon/canon.pcss";
@import "./lib/scena/scena.pcss";

@tailwind base;
@tailwind components;
@tailwind utilities;

@layer base {
  
  html {
    @apply
      /* 4,5,6,7,8 */
      pro-rata-rem--inclusive
      ;
  }

  body {
    @apply
      text-neutral-950 dark:text-neutral-50
      bg-neutral-50 dark:bg-neutral-950
      ;
  }
}